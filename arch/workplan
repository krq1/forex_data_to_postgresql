ALTER TABLE realfun ALTER realDate SET DATA TYPE timestamp USING realDate; 

CREATE TABLE rr AS SELECT to_timestamp(to_char(tb1.date, '9999-99-99 99:99:99'),'YYYY-MM-DD HH24:MI:SS') AS realDate FROM realfun AS tb1; 


WHERE time > CURRENT_TIMESTAMP - INTERVAL '1 minute'
FROM realfunSELECT COUNT (*)WHERE auth_user.lastactivity > CURRENT_TIMESTAMP - INTERVAL '100 days'FROM tableSELECT *  EXTRACT(EPOCH FROM date '2012-10-24' + interval '1 day');  

